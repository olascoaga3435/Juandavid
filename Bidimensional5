//sumar diagonal principal
public class Bidimensional_5 {
    public static void main(String[] args) {
        int[][] sumaDiagonal = {{10, 20, 30}, {40, 50, 60}, {70, 80, 90}};
        int filas = 3, columnas = 3;
        int suma = 0;

        for (int i = 0; i< filas;i++){
            int j = 2; j--;
            suma += sumaDiagonal[i][j];
            }
        System.out.println("La suma diagonal es: "+suma);

        }
}
